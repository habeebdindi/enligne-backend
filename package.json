{
  "name": "enligne-backend",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "start": "node dist/index.js",
    "dev": "ts-node-dev --respawn --transpile-only --exit-child src/index.ts",
    "build": "npx prisma generate && tsc",
    "postinstall": "npx prisma generate",
    "postbuild": "npx prisma migrate deploy",
    "prisma:generate": "npx prisma generate",
    "migrate:dev": "dotenv -e .env.local -- prisma migrate dev",
    "migrate:deploy": "dotenv -e .env -- npx prisma migrate deploy",
    "prisma:seed": "dotenv -e .env.local -- npx ts-node src/seed.ts",
    "prisma:cleanup": "dotenv -e .env.local -- npx ts-node src/cleanup.ts",
    "test": "jest"
  },
  "keywords": [
    "delivery",
    "platform",
    "backend"
  ],
  "author": "",
  "license": "ISC",
  "description": "Backend system for a multi-vendor delivery platform",
  "dependencies": {
    "@prisma/client": "^5.7.0",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-express": "^4.1.3",
    "axios": "^1.10.0",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^16.5.0",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.4",
    "helmet": "^7.1.0",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.0",
    "redis": "^4.6.11",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "4.3.0",
    "ts-node-dev": "^2.0.0",
    "uuid": "^11.1.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.10",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/morgan": "^1.9.9",
    "@types/node": "^20.10.3",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-express": "^4.1.6",
    "jest": "^29.7.0",
    "nodemon": "^3.0.2",
    "prisma": "^5.7.0",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.3.2"
  }
}
